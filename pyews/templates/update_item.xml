{% extends "base_request.xml" %}
{% autoescape None %}
{% block body %}
    <m:UpdateItem ConflictResolution="NeverOverwrite">
      <m:ItemChanges>
        {% for item in items %}
            <t:ItemChange>
              <t:ItemId Id="{{ item.itemid }}" ChangeKey="{{ item.change_key }}" />
              <t:Updates>
    
              {% set adds, sets, dels = item.get_updates() %}
    
              {% for child in adds %}
              {% end %}
    
              {% for child in sets %}
                <t:SetItemField>
                    {{ child.write_to_xml_update() }}
                </t:SetItemField>
              {% end %}
    
              {% for child in dels %}
              {% end %}
    
              </t:Updates>
            </t:ItemChange>
        {% end %}
      </m:ItemChanges>
    </m:UpdateItem>
{% end %}
